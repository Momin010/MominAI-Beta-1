# Server Configuration
NODE_ENV=development
PORT=3001

# Authentication
JWT_SECRET=mominai-jwt-secret-key-change-in-production

# Preview Configuration
PREVIEW_DOMAIN=preview.mominai.com

# Docker Configuration
USE_DOCKER=true
CONTAINER_IMAGE=node:20-bullseye
CONTAINER_CPU_LIMIT=0.5
CONTAINER_MEMORY_LIMIT=512m
DOCKER_NETWORK=ide-network

# Session Management
SESSION_IDLE_TIMEOUT=1800000
MAX_SESSIONS=50

# Workspace Configuration
USER_WORKSPACE_BASE=./user-workspaces

# Security
CORS_ORIGINS=http://localhost:12000,http://127.0.0.1:12000,http://localhost:5173

# Rate Limiting
RATE_LIMIT_WINDOW=900000
RATE_LIMIT_MAX=100

# Logging
LOG_LEVEL=info
LOG_FILE=./logs/server.log